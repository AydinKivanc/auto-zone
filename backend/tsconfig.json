{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es6",
    "rootDir": "."
  },
  "include": [
    "src/**/*.ts",
    "src/types/*.d.ts",
    "src/types/expressdts",
    "types/express.d.ts"
  ],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
